    "build": "vite build --emptyOutDir --outDir ../django_server/static/frontend",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.1.1",
    "@tailwindcss/vite": "^4.1.8",
    "plotly.js": "^3.0.1",
    "plotly.js-dist-min": "^3.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-plotly.js": "^2.6.0",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/react": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.0",
    "eslint": "^9.27.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "vite": "^6.3.5"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
